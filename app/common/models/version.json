{
  "name": "version",
  "description": "Versions of a workspace",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "id": {
      "type": "string",
      "id": true,
      "defaultFn": "uuidv4"
    },
    "slug": {
      "type": "string",
      "required": true,
      "default": "current",
      "description": "Identifier suitable for URLs"
    },
    "owner-slug": {
      "type": "string",
      "description": "Identifier for owner in URLs"
    },
    "workspace-slug": {
      "type": "string",
      "description": "Identifier for owner's workspace in URLs"
    },
    "title": {
      "type": "string",
      "required": true,
      "default": "Current",
      "description": "Title for the version"
    },
    "description": {
      "type": "string",
      "description": "Short description of the version's purpose"
    }
  },
  "validations": [],
  "relations": {
    "iati-datasets": {
      "type": "referencesMany",
      "model": "iati-dataset",
      "foreignKey": "md5"
    }
  },
  "acls": [],
  "methods": {}
}
