{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "iati-file": {
    "dataSource": "iati-filestore",
    "public": true
  },
  "iati-dataset": {
    "dataSource": "iati-datasets",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": true,
          "find": true,
          "findById": true
        }
      }
    }
  },
  "iati-publisher": {
    "dataSource": "iati-publishers",
    "public": true
  },
  "workspace": {
    "dataSource": "workspaces",
    "public": true
  },
  "version": {
    "dataSource": "versions",
    "public": true
  }
}
